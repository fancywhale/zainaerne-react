{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "indent": [
      true,
      "spaces"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-var-requires": false,
    "ordered-imports": false,
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-console": [
      false
    ],
    "jsx-alignment": [
      false
    ],
    "no-unused-locals": [false],
    "no-unused-parameters": [false],
    "no-unused-variable": [false, "check-parameters", "react"]
  }
}
